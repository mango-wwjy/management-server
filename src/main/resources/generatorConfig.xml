<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/manage_system?characterEncoding=utf-8&amp;useSSL=false"
                        userId="root"
                        password="root">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.entity" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="mapper" targetProject="./src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.dao" targetProject="./src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


<!--        <table tableName="user_info" domainObjectName="UserInfo"/>-->
<!--        <table tableName="role_info" domainObjectName="Role"/>-->
        <table tableName="student_info" domainObjectName="Student"/>
        <table tableName="teacher_info" domainObjectName="Teacher"/>
        <table tableName="notice_info" domainObjectName="Notice"/>
        <table tableName="college_info" domainObjectName="College"/>
        <table tableName="subject_info" domainObjectName="Subject"/>
        <table tableName="class_info" domainObjectName="ClassInfo"/>
    </context>

</generatorConfiguration>
